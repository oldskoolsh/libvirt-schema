<domain type='kvm'>
    <name>demo.local</name>
    <uuid>6805a097-b0b8-4bf5-bd9b-d8a7cc326a8c</uuid>
    <metadata xmlns:ns2="http://libosinfo.org/xmlns/libvirt/domain/1.0">
        <ns2:libosinfo>
            <ns2:os id="http://debian.org/debian/12"/>
        </ns2:libosinfo>
    </metadata>
    <memory unit='KiB'>4194304</memory>
    <currentMemory unit='KiB'>4194304</currentMemory>
    <vcpu placement='static'>2</vcpu>
    <resource>
        <partition>/machine</partition>
    </resource>
    <os>
        <type arch='aarch64' machine='virt-6.2'>hvm</type>
        <loader readonly='yes' type='pflash'>/usr/share/AAVMF/AAVMF_CODE.fd</loader>
        <nvram>/var/lib/libvirt/qemu/nvram/demo.local_VARS.fd</nvram>
    </os>
    <features>
        <acpi/>
        <gic version='3'/>
    </features>
    <cpu mode='host-passthrough' check='partial'>
        <topology sockets='1' dies='1' cores='2' threads='1'/>
    </cpu>
    <clock offset='utc'/>
    <on_poweroff>destroy</on_poweroff>
    <on_reboot>restart</on_reboot>
    <on_crash>restart</on_crash>
    <devices>
        <emulator>/usr/bin/qemu-system-aarch64</emulator>
        <disk type='file' device='disk'>
            <driver name='qemu' type='qcow2' discard='unmap'/>
            <source file='/var/lib/libvirt/images/demo.local.qcow2'/>
            <target dev='sda' bus='scsi'/>
            <boot order='1'/>
            <address type='drive' controller='0' bus='0' target='0' unit='0'/>
